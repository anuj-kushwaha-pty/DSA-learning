#include <iostream>
using namespace std;

int main()
{

    int n;
    cin >> n;

    int i = 1;

    while (i <= n)
    {

        int j = 1;
        while (j <=i)
        {
        
            //we just added A-1 in i+j-1
            char ch = i+j-1+'A'-1;
            cout << " "
                 << ch;

            j = j + 1;
        }
        cout << endl;
        i = i + 1;
    }
}
